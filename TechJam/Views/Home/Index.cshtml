@{
    ViewBag.Title = "Home Page";
}
<script>
    $('#nav-overview').addClass('active');
</script>
<div class="row">
    <div class="col-lg-12">
        <h1>Dashboard <small>My health overview</small></h1>
    </div>
</div>
<!-- /.row -->

<div class="row">
    
    <div class="col-lg-3">
        <div class="panel panel-success">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-6">
                        <i class="icon-check icon-5x"></i>
                    </div>
                    <div class="col-xs-6 text-right">
                        <p class="announcement-heading">7</p>
                        <p class="announcement-text">Suggestions</p>
                    </div>
                </div>
            </div>
                <div class="panel-footer">
                    <div class="row">
                        <div class="col-xs-9">
                            Take a 2 mile walk<br />
                            <i class="small">Walking once a day keeps you active.</i>
                        </div>
                        <div class="col-xs-3 text-right">
                            <b class="small">Today</b>
                        </div>
                    </div>
                </div>
                <div class="panel-footer">
                    <div class="row">
                        <div class="col-xs-9">
                            Dental checkup<br />
                            <i class="small">Your annual checkup is due!  Keeping your teeth in good shape prevents gum disease.</i>
                        </div>
                        <div class="col-xs-3 text-right">
                            <b class="small">Nov 6</b>
                        </div>
                    </div>
                </div>
                <div class="panel-footer">
                    <div class="row">
                        <div class="col-xs-9">
                            Nutrition Workshop<br />
                            <i class="small">Learn about how to eat healthy, local, and organic food while also saving money.</i>
                        </div>
                        <div class="col-xs-3 text-right">
                            <b class="small">Nov 11</b>
                        </div>
                    </div>
                </div>
                <div class="panel-footer announcement-bottom">
                    <div class="row">
                        <div class="col-xs-9">
                            See more...
                        </div>
                        <div class="col-xs-3 text-right">
                            <i class="icon-circle-arrow-right"></i>
                        </div>
                    </div>
                </div>
        </div>
    </div>
    <div class="col-lg-3">
        <div class="panel panel-success">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-6">
                        <i class="icon-comments icon-5x"></i>
                    </div>
                    <div class="col-xs-6 text-right">
                        <p class="announcement-heading">22</p>
                        <p class="announcement-text">News Articles</p>
                    </div>
                </div>
            </div>
            @foreach (var news in ViewBag.News) {
            <a href="#">
                <div class="panel-footer">
                    <div class="row" style="margin-top:10px">
                        <div class="col-xs-4">
                            <img src="@news["thumbnail"]" class="img-rounded" />
                        </div>
                        <div class="col-xs-8 text-right">
                            <i><b>Doctor's comments:</b><br />@news["comments"]</i>
                        </div>
                    </div>
                    <div class="row" style="margin-top: 10px">
                        <div class="col-xs-12">
                            <b id="newsArticleTitle"><a href="@news["url"]">@news["title"]</a></b><br />
            <p id="newsArticleDesc">@news["description"]</p>
                        </div>
                    </div>
                </div>
            </a>
            }
                <div class="panel-footer announcement-bottom">
                    <div class="row">
                        <div class="col-xs-9">
                            See more...
                        </div>
                        <div class="col-xs-3 text-right">
                            <i class="icon-circle-arrow-right"></i>
                        </div>
                    </div>
                </div>
        </div>
    </div>
        <div class="col-lg-6">
        <div class="panel panel-primary">
   
                <div id="morris-chart-area"></div>
          
        </div>
    </div>
</div>
<!-- /.row -->


<!-- /.row -->

